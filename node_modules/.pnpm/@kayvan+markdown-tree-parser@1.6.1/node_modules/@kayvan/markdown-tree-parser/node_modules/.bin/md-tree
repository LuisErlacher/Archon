#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/markdown-tree-parser/bin/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/markdown-tree-parser/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/markdown-tree-parser/bin/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/markdown-tree-parser/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules/@kayvan/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/@kayvan+markdown-tree-parser@1.6.1/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/md-tree.js" "$@"
else
  exec node  "$basedir/../../bin/md-tree.js" "$@"
fi
