#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/tools/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/tools/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/bmad-method@6.0.0-alpha.5/node_modules:/home/luis/projetos/Archon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/tools/bmad-npx-wrapper.js" "$@"
else
  exec node  "$basedir/../.pnpm/bmad-method@6.0.0-alpha.5/node_modules/bmad-method/tools/bmad-npx-wrapper.js" "$@"
fi
